{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/_layout.tsx",
		"app/(main)/_layout.tsx",
		"app/(main)/index.tsx",
		"utils/FilterSection.tsx"
	]
}
