{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		},
		"skipLibCheck": true
	},
	"jsx": "react-native",
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"frontend/.expo/types/**/*.ts",
		"expo-env.d.ts",
		"frontend/app/_layout.tsx",
		"frontend/app/(main)/_layout.tsx",
		"frontend/app/(main)/index.tsx",
		"frontend/utils/FilterSection.tsx",
		".expo/types/**/*.ts"
	]
}
